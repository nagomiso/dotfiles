{
    "editor.fontFamily": "'HackGen35 Console for Powerline', 'Rounded M+ 1mn', 'Ricty Diminished Discord', Menlo, Monaco, 'Courier New', monospace",
    "files.eol": "\n",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "python.jediEnabled": false,
    "python.dataScience.sendSelectionToInteractiveWindow": true,
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "autoDocstring.docstringFormat": "numpy",
    "extensions.ignoreRecommendations": false,
    "git.autofetch": true,
    "terminal.integrated.inheritEnv": false,
    "python.analysis.downloadChannel": "daily",
    "python.poetryPath": "/home/jovyan/.poetry/bin/poetry",
    "python.formatting.provider": "black",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.lintOnSave": true,
    "editor.formatOnSave": true,
    "python.linting.flake8Args": [
        "--max-line-length",
        "88",
        "--ignore==E203,W503,W504"
    ],
    "python.testing.pytestEnabled": true,
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.nosetestsEnabled": false,
    "python.analysis.logLevel": "Information",
    "python.analysis.cachingLevel": "System",
    "extensions.autoUpdate": false,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "gitmoji.onlyUseAdditionalEmojis": true,
    "workbench.preferredLightColorTheme": "Monokai",
    "workbench.preferredHighContrastColorTheme": "Monokai",
    "workbench.colorTheme": "Monokai",
    "workbench.preferredDarkColorTheme": "Monokai",
    "workbench.iconTheme": "vscode-icons",
    "vsicons.dontShowNewVersionMessage": true,
    "notebook.kernelProviderAssociations": [
    ],
    "go.useLanguageServer": true,
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        },
        // Optional: Disable snippets, as they conflict with completion ranking.
        "editor.snippetSuggestions": "none",
    },
    "[go.mod]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        },
    },
    "go.languageServerFlags": [
        "-rpc.trace", // for more detailed debug logging
        "serve",
        "--debug=localhost:6060", // to investigate memory usage, see profiles
    ],
    "gopls": {
         // Add parameter placeholders when completing a function.
        "usePlaceholders": true,

        // If true, enable additional analyses with staticcheck.
        // Warning: This will significantly increase memory usage.
        "staticcheck": false,
    },
    "editor.semanticTokenColorCustomizations": null,
    "workbench.colorCustomizations": {
    
    }
}
